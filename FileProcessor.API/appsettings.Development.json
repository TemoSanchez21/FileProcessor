{
  "ConnectionStrings": {
    "FileProcessorDb" : "Server=localhost; User Id=postgres; Password=Dona21; Port=5432; Database=ProcessorFile"
  },
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFramework": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd-MM HH:mm:ss} {Level:u3}] |{SourceContext}| {NewLine} {Message:lj} {NewLine} {Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/FileProcessor-.log",
          "rollingInternal": "Day",
          "rollOnFileSizeLimit": true
        }
      }
    ]
  }
}
